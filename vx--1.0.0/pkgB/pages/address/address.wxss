.body {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.body .noneImg {
  margin-top: 30rpx;
  width: 200rpx;
  height: 200rpx;
}

.body .noneData, .showAll {
  color: #717171;
  margin-top: 20rpx;
  font-size: 28rpx;
}

.body .showAll text {
  margin: 0 20rpx;
}

.body .addBtn {
  width: 85%;
  background-color: #7853B2;
  color: #fff;
  border-radius: 13rpx;
  padding: 20rpx 0;
  margin-top: 200rpx;
  font-weight: 400;
}

.body .address {
  width: 80%;
  padding: 35rpx 25rpx;
  background-color: #fff;
  box-shadow: 0 0 20rpx #a1a1a1;
  margin: 0 auto;
  margin-top: 20rpx;
  display: flex;
  flex-direction: column;
  border-radius: 20rpx;
}

.body .address .address_bottom {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 30rpx;
}

.body .address .address_bottom .bottom_left {
  display: flex;
  align-items: center;
  color: #1067D1;
}

.iconSize {
  width: 30rpx;
  height: 30rpx;
}

.body .address .address_bottom .bottom_right {
  display: flex;
}

.body .address .address_bottom .bottom_right view {
  display: flex;
  align-items: center;
  margin-left: 30rpx;
}

.body .address .address_bottom .bottom_right .delete {
  color: #f00;
}


.ml {
  margin-left: 15rpx;
}